{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES2022",
    "moduleResolution": "Bundler",
    "esModuleInterop": true,
    "lib": ["es2023"],
    "types": ["node"],
    "declaration": true,
    "importHelpers": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "declarationMap": true,
    "composite": true,
  },
}
